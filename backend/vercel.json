{
    "version": 2,
    "name": "payment-app",
    "builds": [
      {
        "src": "backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/routes/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/backend/frontend/dist/$1"
      }
    ]
  }
  